{
    "name": "puli/url-generator",
    "description": "Generates URLs for public Puli resources.",
    "keywords": ["puli", "url", "public", "asset", "resource"],
    "homepage": "http://puli.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernhard Schussek",
            "email": "bschussek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "puli/repository": "1.0.*",
        "puli/discovery": "~1.0",
        "webmozart/glob": "~2.0"
    },
    "require-dev": {
        "puli/cli": "~1.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Puli\\UrlGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Puli\\UrlGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "support": {
        "issues": "https://github.com/puli/issues/issues"
    }
}
